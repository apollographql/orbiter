{
  "name": "orbiter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run codegen && tsc",
    "clean": "rm ./node_modules ./package-lock.json ./tscsonfig.tsbuildinfo ./dist ./generated ./.nvmrc -rf && npm run node-version",
    "watch": "concurrently 'tsc --watch' 'npm run codegen -- --watch'",
    "start": "npm run build && concurrently 'tsc --watch' 'npm run codegen -- --watch' 'netlify dev'",
    "quickstart": "tsc && netlify dev",
    "test": "jest",
    "codegen": "graphql-codegen",
    "format": "prettier --write './src/**/*.ts' --ignore-path ./src/generated/studio.ts",
    "node-version": "node -e 'console.log(require(\"./package.json\").volta.node)' > .nvmrc"
  },
  "author": "opensource@apollographql.com",
  "license": "MIT",
  "devDependencies": {
    "babel-jest": "^26.6.3",
    "concurrently": "^6.0.2",
    "jest": "^26.6.3",
    "netlify-cli": "^3.24.3",
    "nock": "^13.0.11",
    "prettier": "^2.5.1"
  },
  "dependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-document-nodes": "^2.2.5",
    "@graphql-codegen/typescript-graphql-request": "^4.3.7",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@types/aws-lambda": "^8.10.57",
    "@types/babel__core": "^7.1.19",
    "@types/babel__generator": "^7.6.4",
    "@types/btoa": "^1.2.3",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^12.12.47",
    "@types/node-fetch": "^2.5.7",
    "@types/uuid": "^8.3.0",
    "@sentry/serverless": "^6.2.1",
    "btoa": "^1.2.1",
    "graphql": "^15.8.0",
    "graphql-request": "^4.0.0",
    "make-fetch-happen": "^8.0.14",
    "typescript": "^4.5.5",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": ">=14 <15",
    "npm": "6.x"
  },
  "volta": {
    "node": "14.19.1"
  }
}
